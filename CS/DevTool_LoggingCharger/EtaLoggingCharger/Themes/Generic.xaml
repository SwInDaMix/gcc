<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:EtaLoggingCharger">
    <Style TargetType="{x:Type local:EtaCtlGraph}">
        <Setter Property="Background" Value="#FF202020" />
        <Setter Property="GridRectanglePenBrush" Value="#FF808080" />
        <Setter Property="GridLinesPenBrush" Value="#FF303030" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:EtaCtlGraph}">
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
